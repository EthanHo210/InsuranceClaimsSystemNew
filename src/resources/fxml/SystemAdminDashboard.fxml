<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="JavaFiles.SystemAdminDashboardController">
    <Label text="System Admin Dashboard" style="-fx-font-size: 20px;"/>
    <TableView fx:id="dependentTable">
        <columns>
            <TableColumn text="User ID" fx:id="userIdColumn" prefWidth="100"/>
            <TableColumn text="Username" fx:id="usernameColumn" prefWidth="150"/>
            <TableColumn text="Email" fx:id="emailColumn" prefWidth="200"/>
            <TableColumn text="Phone" fx:id="phoneColumn" prefWidth="150"/>
            <TableColumn text="Address" fx:id="addressColumn" prefWidth="150"/>
            <TableColumn text="Claim ID" fx:id="claimIdColumn" prefWidth="100"/>
            <TableColumn text="Status" fx:id="statusColumn" prefWidth="100"/>
            <TableColumn text="Description" fx:id="descriptionColumn" prefWidth="200"/>
            <TableColumn text="Date Filed" fx:id="dateFiledColumn" prefWidth="150"/>
            <TableColumn text="Date Processed" fx:id="dateProcessedColumn" prefWidth="150"/>
        </columns>
    </TableView>
</VBox>