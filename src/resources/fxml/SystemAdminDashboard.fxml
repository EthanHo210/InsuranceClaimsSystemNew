<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="JavaFiles.SystemAdminDashboardController">
    <Label text="System Admin Dashboard" style="-fx-font-size: 20px;"/>
    <Button text="Manage Users"/>
    <TableView fx:id="usersTable">
        <columns>
            <TableColumn text="JavaFiles.User ID" fx:id="userIdColumn" prefWidth="100"/>
            <TableColumn text="Username" fx:id="usernameColumn" prefWidth="150"/>
            <TableColumn text="Role" fx:id="roleColumn" prefWidth="100"/>
        </columns>
    </TableView>
</VBox>
