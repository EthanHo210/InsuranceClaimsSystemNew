<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<VBox alignment="CENTER" spacing="10" xmlns:fx="http://javafx.com/fxml" fx:controller="JavaFiles.DependentDashboardController">
    <Label text="JavaFiles.Dependent Dashboard" style="-fx-font-size: 20px;"/>
    <Button text="View Dependents"/>
    <TableView fx:id="dependentsTable">
        <columns>
            <TableColumn text="JavaFiles.Dependent ID" fx:id="dependentIdColumn" prefWidth="100"/>
            <TableColumn text="Name" fx:id="nameColumn" prefWidth="150"/>
            <TableColumn text="Relationship" fx:id="relationshipColumn" prefWidth="100"/>
        </columns>
    </TableView>
</VBox>
